  "browserslist": [
    "defaults",
    "not ie 11",
    "not dead"
  ],
  "dependencies": {
    "@googlemaps/js-api-loader": "^2.0.1",
    "carbon-geomap": "^0.2.0",
    "leaflet": "^1.9.4",
    "maplibre-gl": "^5.8.0",
    "nanoid": "^5.1.6",
    "protomaps-leaflet": "^4.1.1",
    "protomaps-themes-base": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@prettier/plugin-php": "^0.24.0",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.1.1",
    "esbuild": "^0.25.10",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
